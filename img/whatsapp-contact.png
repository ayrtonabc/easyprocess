<svg xmlns="http://www.w3.org/2000/svg" width="500" height="500" viewBox="0 0 500 500">
  <!-- Fondo del teléfono -->
  <rect x="150" y="50" width="200" height="400" rx="20" fill="#f0f0f0" />
  
  <!-- Pantalla del teléfono -->
  <rect x="160" y="80" width="180" height="340" rx="5" fill="#ffffff" stroke="#e0e0e0" stroke-width="2" />
  
  <!-- Barra de estado -->
  <rect x="160" y="80" width="180" height="30" rx="5" fill="#075e54" />
  
  <!-- Logo de WhatsApp -->
  <circle cx="250" cy="180" r="40" fill="#25D366" />
  <path d="M230,170 L240,190 L270,160" stroke="white" stroke-width="8" fill="none" stroke-linecap="round" stroke-linejoin="round" />
  
  <!-- Burbujas de chat -->
  <rect x="180" y="240" width="100" height="30" rx="15" fill="#e1ffc7" />
  <rect x="220" y="280" width="100" height="30" rx="15" fill="#e1ffc7" />
  <rect x="180" y="320" width="120" height="30" rx="15" fill="#e1ffc7" />
  
  <!-- Icono de WhatsApp grande -->
  <circle cx="380" cy="120" r="60" fill="#25D366" />
  <path d="M380,100 C365,100 350,110 350,130 C350,150 365,160 380,160 C395,160 410,150 410,130 C410,110 395,100 380,100 Z" fill="white" />
  <path d="M365,125 L375,135 L395,115" stroke="#25D366" stroke-width="8" fill="none" stroke-linecap="round" stroke-linejoin="round" />
  
  <!-- Texto "WhatsApp" -->
  <text x="250" y="400" font-family="Arial, sans-serif" font-size="24" font-weight="bold" text-anchor="middle" fill="#075e54">WhatsApp</text>
  
  <!-- Círculos decorativos -->
  <circle cx="100" cy="150" r="20" fill="#25D366" opacity="0.3" />
  <circle cx="120" cy="350" r="30" fill="#25D366" opacity="0.2" />
  <circle cx="400" cy="300" r="25" fill="#25D366" opacity="0.3" />
  <circle cx="350" cy="400" r="15" fill="#25D366" opacity="0.2" />
</svg>